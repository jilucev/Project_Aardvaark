= form_for @event, url: organization_event_path do |f|
  = f.label "Date" 
  %br
  = f.date_field :date
  %br
  = f.label "Start Time"
  %br
  = f.time_field :event_time
  %br
  = f.label "Location"
  %br
  = f.text_field :location
  %br
  = f.label "Description"
  %br
  = f.text_area :description
  %br
  = f.submit "Submit"